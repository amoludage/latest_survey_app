= flash[:notice]
= flash[:error]
%h1
  Login

= form_tag session_create_path do
  = label_tag :user_name
  = text_field_tag :email
  %br
  = label_tag :password
  = password_field_tag :password
  %br
  = submit_tag 'Login'
  %br

%p
  New user? please register first
= link_to "Register", register_new_path
