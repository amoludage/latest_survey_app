%p
  User information

= form_for @user do |user|
  = user.label :name
  = user.text_field :name
  %br
  = user.label :password
  = user.text_field :password
  %br
  = user.label :email
  = user.text_field :email
  %br
  = user.label :gender
  = user.select :gender, User::GENDER
  %br
  = user.label :age
  = user.text_field :age
  %br
  .actions
    = user.submit

= link_to 'Login page', users_path
